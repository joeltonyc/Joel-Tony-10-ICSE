/**Accept 15 integers
	Perform selection sort in descending order**/

import java.util.*;
public class Temp2 {
	
	public static Scanner sc = new Scanner(System.in);
	
	public static void main(String[] args) {
		int a[] = new int [10];
		int b[] = new int [10];
		for (int i = 0; i < 10; i++) {
			System.out.print("Numbers: ");
			a[i] = sc.nextInt();
			b[i] = sc.nextInt();
		}
		for (int i = 0; i < 10; i++) {
			int s = a[i] + b[i];
			System.out.println(s);
		}
	}
}