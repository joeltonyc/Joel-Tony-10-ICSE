import java.util.*;
public class Three {
    
    public static Scanner sc = new Scanner(System.in);
    
    public static void main(String[] args) {
        j.p("n = ");
        int n = sc.nextInt();
        int a[] = new int[n];
        int b[] = new int[n];
        int c[] = new int[n];
        for (int i = 0; i < n; i++) {
            j.p("Number for 1st array: ");
            a[i] = sc.nextInt();
            j.p("Number for 2nd array: ");
            b[i] = sc.nextInt();
            j.pl();
            
            c[i] = a[i] + b[i];
        }
        for (int i = 0; i < n; i++) {
            System.out.print(c[i] + "\t");
        }
    }
}