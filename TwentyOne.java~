/**Take a 2*3 array
	Replace every even element with i^2
	Replace every odd element with i^3**/

import java.util.*;
public class TwentyOne {
	
	public static Scanner sc = new Scanner(System.in);
	
	public static void main(String[] args) {
		int a[][] = new int [2][3];
		int b[][] = new int [2][3];
		for (int i = 0; i < 2; i++) {
			for (int x = 0; x < 3; x++) {
				j.p("Number: ");
				a[i][x] = sc.nextInt();
			}
		}
		for (int i = 0; i < 2; i++) {
			for (int x = 0; x < 3; x++) {
				int n = a[i][x];
				b[i][x] = (n % 2 == 0) ? (n * n) : (n * n * n);
			}
		}
		for (int i = 0; i < 2; i++) {
			for (int x = 0; x < 3; x++) {
				System.out.println(b[i][x]);
			}
		}
	}
}